    <authors>Ghaus Iftikhar</authors>
    <packageSourceUrl>https://github.com/Kristinita/SashaChocolatey/tree/master/packages/Jumpshare</packageSourceUrl>
    <projectUrl>https://jumpshare.com/</projectUrl>
    <licenseUrl>https://jumpshare.com/terms</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://cdn.rawgit.com/Kristinita/SashaChocolatey/master/icons/Jumpshare.png</iconUrl>
